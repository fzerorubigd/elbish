{% extends 'master.twig' %}

{% block title %}{{ collection.title }} - Page {{ pager.getCurrentPage }}{% endblock %}

{% block content %}
    {% for post in posts %}
        <a href="{{ post._url }}">{{ post.title }}</a>
        <a href="{{ config['site.base_url'] }}{{ post['_url'] }}">abs</a>
        {{ post.getText|raw }}
    {% endfor %}
{% endblock %}