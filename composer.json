{
    "name": "fzerorubigd/elbish",
    "description": "A simple static-content publishing system",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "fzerorubigd/yaml-front-matter": "dev-master",
        "symfony/yaml": "~2.3",
        "kriswallsmith/assetic": "~1.1",
        "twig/twig": "~1.15",
        "michelf/php-markdown": "~1.3",
        "symfony/console": "~2.4",
        "romaricdrigon/metayaml": "dev-master",
        "symfony/finder": "~2.4",
        "nikic/php-parser": "0.9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "mockery/mockery": "dev-master@dev"
    },
    "license": "MIT",
    "config": {
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-0": {
            "Cybits": "src/"
        }
    },
    "authors": [
        {
            "name": "fzerorubigd",
            "email": "fzerorubigd@gmail.com"
        }
    ]
}
